<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Dress">
<!-- 	옷 등록 -->
	<insert id="insert" parameterType="java.util.Map">
		insert into dress values(dress_seq.nextval,#{c_no},#{email},#{name},#{category},#{pub},#{season},sysdate,#{memo},#{price},#{buy_date})
	</insert>
<!-- 	옷 이미지 등록 -->
	<insert id="insertImg">
		insert into dress_img values(dress_img_seq.nextval,#{d_no},#{ori_name},#{sys_name},#{path})
	</insert>
<!-- 	옷 번호 가져오기 -->
	<select id="selectNo" resultType="kh.spring.dto.DressDTO">
		select * from dress order by 1 desc
	</select>
	
	
</mapper>